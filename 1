#include<stdio.h>
 
void multiplication (int a);

int main ()
{
	int i, j, a;
	while(scanf("%d", &a) !=EOF)
	multiplication (a);

	return 0;
}

void multiplication (int a)
{
	int i, j;
	for (i = 1; i <= a; i++)
	{
		for (j = 1; j <= i; j++)
		{
			printf ("%d * %d = %d  ", i, j, i*j);
			if (i == j)
				printf ("\n");
		}
	}
}
