#include<stdio.h>
 
void exchange (int a, int b);

int main ()
{
	int a, b;
	while (scanf ("%d %d", &a, &b) != EOF)
    {
		printf("before：a=%d  b=%d\n\n", a, b);
		exchange (a, b);
	}
	return 0;
}

void exchange (int a, int b)
{
	a = a ^ b;
	b = a ^ b;
	a = a ^ b;
	printf("after：a=%d  b=%d\n", a, b);
}
